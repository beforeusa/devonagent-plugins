<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Name</key>
	<string>RFC Search</string>
	<key>Info</key>
	<string>RFC Search Plugin Â©2009 Stateful Labs</string>
	<key>Description</key>
	<string>Searches for RFC documents containing search term(s).</string>
	<key>Version</key>
	<string>1.0</string>
	<key>Identifier</key>
	<string>net.stateful.devonagent.rfcsearch.plugin</string>
